<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_app"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.grupoqq.app.fragments.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_searchBinnacleFragment"
            app:destination="@id/searchBinnacleFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchBinnacleFragment"
        android:name="com.grupoqq.app.fragments.SearchBinnacleFragment"
        android:label="fragment_search_binnacle"
        tools:layout="@layout/fragment_search_binnacle" >
        <action
            android:id="@+id/action_searchBinnacleFragment_to_binnacleFragment"
            app:destination="@id/binnacleFragment" />
    </fragment>
    <fragment
        android:id="@+id/binnacleFragment"
        android:name="com.grupoqq.app.fragments.BinnacleFragment"
        android:label="fragment_binnacle"
        tools:layout="@layout/fragment_binnacle" />

</navigation>